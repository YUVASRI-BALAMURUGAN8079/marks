#include<stdio.h>
int main()
{
	int mark;
	char name;
	printf("Enter the student name:");
	scanf("%s",&name);
	printf("Enter the mark:");
	scanf("%d",&mark);
	if(mark>=85)
	{
		printf("Grade A");
	}
	if(mark<=84 && mark>=70)
	{
		printf("Grade B");
	}
	if(mark>=55 && mark<=69)
	{
		printf("Grade C");
	}
	if(mark>=40 && mark<=54)
	{
		printf("Grade D");
	}
	if(mark<40)
	{
		printf("Grade F");
	}
	return 0;
}
